    "import subprocess\n",
    "\n",
    "plots = [f\"problem{i}.png\" for i in range(1, 5)]\n",
    "\n",
    "# Generate a high-quality PDF from PNG images\n",
    "cmd = [\"convert\", \"-density\", \"300\"] + plots + \\\n",
    "    [\"-compress\", \"jpeg\", \"-quality\", \"95\", \"problem.pdf\"]\n",
    "\n",
    "# Execute the command\n",
    "subprocess.run(cmd, check=True)"
   ]
